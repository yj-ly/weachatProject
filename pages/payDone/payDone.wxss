page{
  width: 100%;
  height: 100%;
}
.main{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.icon{
  width: 222rpx;
  height: 206rpx;
  display: flex;
  flex-direction: row;
  margin: 80rpx auto 46rpx auto;
}
.icon image{
  width: 100%;
  height: 100%;
}
.title{
  font-size: 40rpx;
  color: #414141;
  text-align: center;
  margin-bottom: 52rpx;
}
.money{
  font-size: 40rpx;
  color: #363636;
  text-align: center;
  margin-bottom: 36rpx;
}
.money_icon{
  font-size: 23rpx;
}
.orderStatus{
  color: #9e9e9e;
  font-size: 28rpx;
  text-align: center;
  margin-bottom: 72rpx;
}
.buttons{
  width: 100%;
  height: 78rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.buttons_but{
  width: 238rpx;
  height: 78rpx;
  line-height: 78rpx;
  text-align: center;
  color: #076299;
  font-size: 30rpx;
  border: 1rpx solid #076299;
  border-radius: 5rpx;
}
.buttons_but:first-child{
  margin-right: 118rpx;
}
.buttons_but:last-child{
  background: #076299;
  color: #fff;
}